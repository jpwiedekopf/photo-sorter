{
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-i=/Volumes/T7/DCIM/117_FUJI",
                "-i=/Volumes/T7/test-in",
                "-o=/Volumes/T7/out",
                "-o=/Volumes/T7/out2"
            ],
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        }
    ]
}